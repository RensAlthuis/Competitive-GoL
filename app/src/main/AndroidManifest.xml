<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rens.competitive_gol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.MainActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.RoomActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.SettingsActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.TutorialActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.RatingActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.FeedBackActivity" />
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.AboutActivity"></activity>
        <activity android:screenOrientation="portrait" android:name=".Controller.Activities.WinLossActivity"></activity>

    </application>

</manifest>